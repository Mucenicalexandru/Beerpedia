operties.key;\n    const anotherKey = properties.anotherKey;\n\n    useEffect(() =>{\n        axios.get(`https://sandbox-api.brewerydb.com/v2/beers?key=${key}&p=${currentPage}`)\n            .then(res => {\n                setBeerData(res.data.data)\n            })\n    }, [currentPage])\n\n    return (\n        <>\n            <BeerContext.Provider value={beerData}>\n                {props.children}\n            </BeerContext.Provider>\n            <nav aria-label=\"Page navigation example\">\n                <ul className=\"pagination\">\n                    <li className=\"page-item\"><ButtonStyle className=\"page-link\" onClick={() => {\n                        setCurrentPage(currentPage - 1)\n                        setPageNumber(pageNumber-1)\n                    }}>Previous</ButtonStyle></li>\n\n                    <li className=\"page-item\">\n                        <a className=\"page-link\" href=\"#\">\n                            {pageNumber-1}\n                        </a>\n                    </li>\n\n                    <li className=\"page-item\">\n                        <CurrentPage className=\"page-link\" href=\"#\">\n                            {pageNumber}\n                        </CurrentPage>\n                    </li>\n\n                    <li className=\"page-item\">\n                        <a className=\"page-link\" href=\"#\">\n                            {pageNumber+1}\n                        </a>\n                    </li>\n\n                    <li className=\"page-item\"><ButtonStyle className=\"page-link\" onClick={() => {\n                        setCurrentPage(currentPage + 1)\n                        setPageNumber(pageNumber + 1)\n                    }}>Next</ButtonStyle></li>\n                </ul>\n            </nav>\n        </>\n    );\n}\n","/home/alex/Desktop/projects/advanced/brew/src/layout/Homepage.js",[],"/home/alex/Desktop/projects/advanced/brew/src/layout/Header.js",["89"],"/home/alex/Desktop/projects/advanced/brew/