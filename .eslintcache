body\">\n                                    <h3 className=\"card-text\">Established : {brewery.established}</h3>\n                                    <p className=\"card-text\">{brewery.description}</p>\n                                    <Link to={`/brewery/${brewery.id}/beers`}>See beers produced by {brewery.name}</Link>\n                                </div>\n                            </BreweriesCard>\n                        </div>\n                    }\n                }\n\n            })};\n\n        </>\n    );\n}\n\nconst TextBlock = styled.div`\n  color: #0a0a4c;\n  margin: 100px;\n  padding: 20px;\n  background-color: #fff4e3;\n  border: #c6c1c1 solid 1px;\n`\n\nconst BreweriesCard = styled.div`\nmargin-left: 200px;\nmargin-bottom: 100px;\nbackground-color: #d0e8e8;\nborder-radius: 10px;\nwidth: 75%;\n`\n\nconst Image = styled.img`\nmargin-right: 20px;\nwidth: 25%;\n`\n\nconst CardHeader = styled.h5`\nbackground-color: #c2e5e5;;\n`\n\nexport default Breweries;","/home/alex/Desktop/projects/advanced/brew/src/properties.js",[],"/home/alex/Desktop/projects/advanced/brew/src/layout/BeersByBrewery.js",[],"/home/alex/Desktop/projects/advanced/brew/src/components/MoreInfo.js",["97","98","99"],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":3,"column":9,"nodeType":"106","messageId":"107","endLine":3,"endColumn":12},{"ruleId":"104","severity":1,"message":"108","line":3,"column":14,"nodeType":"106","messageId":"107","endLine":3,"endColumn":25},{"ruleId":"104","severity":1,"message":"109","line":3,"column":27,"nodeType":"106","messageId":"107","endLine":3,"endColumn":31},{"ruleId":"104","severity":1,"message":"110","line":3,"column":33,"nodeType":"106","messageId":"107","endLine":3,"endColumn":44},{"ruleId":"104","severity":1,"message":"111","line":3,"column":46,"nodeType":"106","messageId":"107","endLine":3,"endColumn":52},{"ruleId":"112","severity":1,"message":"113","line":45,"column"